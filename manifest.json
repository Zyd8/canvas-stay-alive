{
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "name": "Canvas, Stay Alive!",
  "version": "1.0",
  "description": "Stops canvas.com from the user being Timed Out",
  "permissions": [
    "tabs",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
